{% extends 'templates/base.jinja2' %}

{% block headers %}

    {% set title = 'Playground - Text Generator - Make interactive text experiences' -%}
    {% set description = 'Try advanced AI text generation API' -%}

    <meta charset="utf-8">
    <title>{{ title }}</title>
    <meta name="description"
          content="{{ description }}">

    <meta name="keywords" content="ai bot, text generator, OpenAI, GPT">
    <meta property="og:title" content="{{ title }}">
    <meta property="og:url" content="{{ url }}">
    <meta property="og:image" content="{{ static_url }}/img/brain-generated256.png">
    <meta property="og:description"
          content="{{ description }}">
    <meta property="og:type" content="website">
    <meta property="og:site_name" content="Text Generator">
    <meta property="fb:admins" content="337972272904903">
    <link rel="canonical" href="https://text-generator.io/playground">

{% endblock %}

{% block mainbody %}
    {% include "/templates/shared/playground.jinja2" %}
{% endblock %}


