{% extends 'templates/base.jinja2' %}

{% block headers %}

    {% set title = 'Self Hosting Text Generator - Create Text Experiences and bots' -%}
    {% set description = 'How to generate realistic text locally! OpenAI Compatible Affordable Quality Text Generation' -%}

    <meta charset="utf-8">
    <title>{{ title }}</title>
    <meta name="description"
          content="{{ description }}">

    <meta name="keywords" content="ai bot, text generator, OpenAI, GPT">
    <meta property="og:title" content="{{ title }}">
    <meta property="og:url" content="https://text-generator.io">
    <meta property="og:image" content="{{ static_url }}/img/brain-generated256.png">
    <meta property="og:description"
          content="{{ description }}">
    <meta property="og:type" content="website">
    <meta property="og:site_name" content="Text Generator">
    <meta property="fb:admins" content="337972272904903">

{% endblock %}

{% block mainbody %}
    {% include "/templates/shared/selfhosting.jinja2" %}
{% endblock %}
