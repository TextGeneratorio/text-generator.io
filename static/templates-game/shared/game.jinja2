        <div>
            <div class="col-md-6">
                <div class="mm-game-wrapper">
                    <div class="mm-game-header">
                        <div class="mm-game-header-title">
                            <h1></h1>
                        </div>
                    </div>
                </div>
            </div>
            <div class="main">
                <div class="container ">
                    <div class="chat-log">
                        <div class="chat-log__item">
                            <h3 class="chat-log__author">Alfred Bottington</h3>
                            <div class="chat-log__message"> <span class="typer" id="typer-one"
                                                                  data-words="Try to guess what I'm thinking of! You can ask me yes or no questions, or just say whatever you like." data-delay="50"
                                                                  data-loop="1" data-delim="-------"
                            ></span></div>
                        </div>

