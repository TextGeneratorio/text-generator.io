{% extends 'templates-game/base.jinja2' %}

{% block headers %}

    {% set title = 'Terms | reWord Game The Massive Chess Game' -%}
    {% set description = 'Terms and Conditions | reWord Game! The Utterly Massive Chess Game, Fun For Everyone!' -%}

    <meta charset="utf-8">
    <title>{{ title }}</title>
    <meta name="description"
          content="{{ description }}">

    <meta name="keywords" content="puzzle games, chess terms, online chess, game, multiplayer chess game">
    <meta property="og:title" content="{{ title }}">
    <meta property="og:url" content="http://rewordgame.com">
    <meta property="og:image" content="http://rewordgame.com/static/img/big-multiplayer-chess-logo256.png">
    <meta property="og:description"
          content="{{ description }}">
    <meta property="og:type" content="game">
    <meta property="og:site_name" content="reWord Game">
    <meta property="fb:admins" content="337972272904903">

{% endblock %}

{% block mainbody %}
    {% include "/templates-game/shared/terms.jinja2" %}
{% endblock %}


